<% if @permission.new_record? %>
  alert('The file could not be uploaded: <%= j @permission.errors.full_messages.join(', ').html_safe %>');
<% else %>
  $('#permission-list-user-<%= @permission.user_id %>').append("<%= j (render 'permissions/permission_list_item', permission: @permission, user: @user) %>");
  $('#folder-<%= @permission.folder_id %>').hide();
<% end %>